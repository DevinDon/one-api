{
  "name": "@iinfinity/one-api",
  "version": "0.3.0",
  "description": "One API.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "MODE=DEV ./scripts/build",
    "build:prod": "MODE=PROD ./scripts/build",
    "clean": "./scripts/clean",
    "debug": "./scripts/debug",
    "postinstall": "npm run init",
    "init": "./scripts/init",
    "lint": "./scripts/lint",
    "pack": "npm run clean && npm run build && npm run push",
    "push": "./scripts/push",
    "service": "./scripts/service",
    "start": "npm run start:dev",
    "start:dev": "MODE=DEV ./scripts/start",
    "start:prod": "MODE=PROD ./scripts/start",
    "start:demo": "ts-node src/demo.ts",
    "test": "./scripts/test"
  },
  "keywords": [
    "one",
    "one-api",
    "typescript"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red/)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "html-to-md": "^0.4.3",
    "node-fetch": "^2.6.1"
  },
  "devDependencies": {
    "@types/jasmine": "3.8.1",
    "@types/node": "16.3.0",
    "@types/node-fetch": "^2.5.11",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "cross-env": "7.0.3",
    "esbuild": "0.12.15",
    "eslint": "7.30.0",
    "jasmine": "3.8.0",
    "jasmine-console-reporter": "3.1.0",
    "ts-loader": "9.2.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.5"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/one-api.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/one-api/issues"
  },
  "homepage": "https://github.com/DevinDon/one-api"
}

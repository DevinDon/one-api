{
  "name": "@iinfinity/one-api",
  "version": "0.2.0",
  "description": "One API.",
  "main": "dist/index.js",
  "directories": {
    "doc": "docs"
  },
  "files": [
    "dist",
    "docs"
  ],
  "scripts": {
    "build": "bash script/build.sh",
    "clean": "bash script/clean.sh",
    "lint": "eslint .",
    "pack": "npm run build && npm run push",
    "push": "bash script/push.sh",
    "prepublishOnly": "npm run clean && tsc",
    "start": "npm run start:dev",
    "start:debug": "node --inspect-brk -r ts-node/register src/main/index.ts",
    "start:demo": "MODE=DEV node -r ts-node/register src/demo/index.ts",
    "start:dev": "MODE=DEV node -r ts-node/register src/main/index.ts",
    "start:prod": "npm run build && MODE=PROD node dist",
    "test": "node node_modules/jasmine/bin/jasmine.js --config=src/test/jasmine.json --reporter=jasmine-console-reporter --require=ts-node/register",
    "watch": "tsc -w"
  },
  "keywords": [
    "typescript",
    "project"
  ],
  "author": "IInfinity <I.INF@Outlook.com> (https://don.red/)",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@iinfinity/logger": "^0.3.4",
    "axios": "^0.21.1",
    "html-to-md": "^0.3.6"
  },
  "devDependencies": {
    "@types/jasmine": "3.x",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^4.14.0",
    "@typescript-eslint/parser": "^4.14.0",
    "eslint": "^7.18.0",
    "jasmine": "3.x",
    "jasmine-console-reporter": "3.x",
    "ts-node": "9.x",
    "typescript": "4.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DevinDon/one-api.git"
  },
  "bugs": {
    "url": "https://github.com/DevinDon/one-api/issues"
  },
  "homepage": "https://github.com/DevinDon/one-api"
}
